{
	"version": "0.1.3",
	"manifest_version": 2,
	"minimum_chrome_version": "33",
	"name": "nad11util",
	"description": "NAD11 utility",
	"icons": 
	{
		"16": "assets/icon.png",
		"128": "assets/icon.png"
	},

	"app": 
	{
		"background": 
		{
			"scripts": 
			[
				"background.js"
			]
		}
	},

	"offline_enabled": true,
	"permissions": 
	[
		"notifications",
		"storage",
		"alwaysOnTopWindows",
		"http://aterm.me/index.cgi/*"
	]
}